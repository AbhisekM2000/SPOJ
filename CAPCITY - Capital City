#include <bits/stdc++.h>
using namespace std;

#define pb push_back
#define ff first
#define ss second
#define mp make_pair
#define memo(a,b) memset(a,b,sizeof(a))
#define INF 1e9
#define EPS 1e-8
#define PI 3.14159265358979323846

typedef long long ll ;
typedef unsigned long long ull ;

/* int dx[] = {1,-1,0,0} , dy[] = {0,0,1,-1}; */ // 4 Direction
/* int dx[] = {1,-1,0,0,1,1,-1,-1} , dy[] = {0,0,1,-1,1,-1,1,-1}; */ // 8 Direction
/* int dx[] = {1,-1,1,-1,2,2,-2,-2} , dy[] = {2,2,-2,-2,1,-1,1,-1}; */ // Knight Direction
/* int dx[] = {2,-2,1,1,-1,-1} , dy[] = {0,0,1,-1,1,-1}; */ // Hexagonal Direction
int N , M , dt[100000+10] , ft[100000+10] , ct[100000+10] , cycle = 1 , ti = 1 , test = 0 ;
bool visit[100000+10] ;
vector <int> G[100000+10] , GT[100000+10] , C[100000+10] , CT[100000+10] , Q ;
void dfs1(int u)
{
    visit[u] = true ;
    dt[u] = ti++ ;
    for(int i=0;i<G[u].size();i++)
    {
        int v = G[u][i] ;
        if(!visit[v]) dfs1(v);
    }
    ft[u] = ti++ ;
    Q.pb(u);
}
void dfs2(int u,int cmp)
{
    visit[u] = false ;
    ct[u] = cycle ;
    //cout << u << ' ' << ct[u] << endl ;
    for(int i=0;i<GT[u].size();i++)
    {
        int v = GT[u][i] ;
        if(visit[v]&&cmp>ft[v]) dfs2(v,cmp);
    }
}
void dfsCycle(int u)
{
    visit[u] = true ;
    for(int i=0;i<G[u].size();i++)
    {
        int v = G[u][i] ;
        int x = ct[u] , y = ct[v] ;
        if(x!=y)
        {
            C[x].pb(y) ;
            CT[y].pb(x) ;
        }
        if(!visit[v]) dfsCycle(v);
    }
}
int findLast(int u)
{
    if(CT[u].size()==0) return u ;
    else return findLast(CT[u][0]) ;
}
int againfindLast(int u)
{
    if(C[u].size()==0) return u ;
    else return againfindLast(C[u][0]) ;
}
void dfs(int u)
{
    visit[u] = true ;
    for(int i=0;i<CT[u].size();i++)
    {
        int v = CT[u][i] ;
        if(!visit[v]) dfs(v);
    }
}
bool ok(int u)
{
    dfs(u);
    for(int i=1;i<cycle;i++) if(visit[i]==false) return false ;
    return true ;
}
int main()
{
    //freopen("input.txt","r",stdin);
    //freopen("output.txt","w",stdout);
    scanf("%d%d",&N,&M);
    while( M-- )
    {
        int u , v ;
        scanf("%d%d",&u,&v);
        G[u].pb(v);
        GT[v].pb(u);
    }
    for(int i=1;i<=N;i++) if(!visit[i]) dfs1(i);
    for(int i=Q.size()-1;i>=0;i--) if(visit[Q[i]]) { dfs2(Q[i],ft[Q[i]]); cycle++; }
    for(int i=1;i<=N;i++) if(!visit[i]) dfsCycle(i);
    int t = findLast(1);
    t = againfindLast(t);
    memo(visit,false);
    if(ok(t))
    {
        vector <int> V ;
        for(int i=1;i<=N;i++)
        {
            if(ct[i]==t) V.pb(i);
        }
        cout << V.size() << endl ;
        for(int i=0;i<V.size();i++)
        {
            if(i!=0) printf(" ");
            printf("%d",V[i]);
        }
    }
    else puts("0");
    return 0;
}
